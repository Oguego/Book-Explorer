<div class="row justify-content-center">
  <div class="col-sm-12 col-md-8">

    <div class="card card-fluid">
      <h6 class="card-header">
        <span class="badge badge-pill badge-info"><h3>CSV Files</h3></span> <br>
        CSV File Information
      </h6>

      <div class="card-body" >
        <%= render '/csv_files/display_csv', csv_files: @csv_files %>
      </div>

      <div class="card-footer text-right">
        <%= link_to "Upload CSV File", new_csv_file_path, class: 'btn btn-outline-primary'  %> |
        <%= link_to "Home", home_page_path, class: 'btn btn-outline-primary'  %>
      </div>

    </div>
  </div>
</div>
